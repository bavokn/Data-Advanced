    "    def drawPieChart(self, fileName):\n",
    "        wb = load_workbook(fileName)\n",
    "        ws = wb['gegevens']\n",
    "\n",
    "        data = {\"matig\": 0, \"goed\": 0, \"zeer goed\": 0}\n",
